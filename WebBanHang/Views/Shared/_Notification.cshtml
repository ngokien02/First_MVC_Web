@if (TempData["success"] != null)
{
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
	<script>
		Swal.fire({
		title: "@TempData["success"]",
			text: "success!",
		icon: "success"
		});
	</script>
}
@if (TempData["error"] != null)
{
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
	<script>
		Swal.fire({
		title: "@TempData["error"]",
			text: "success!",
		icon: "error"
		});
	</script>
}